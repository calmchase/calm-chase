{
  "name": "calm-chase",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "author": "Raj",
  "description": "Calm chase landing page",
  "lint-staged": {
    "*/**/*.{json,css,md}": [
      "prettier --write"
    ],
    "*/**/*.{mjs,cjs,js,ts,svelte}": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "pnpm": ">= 9.0.0",
    "npm": "Please use pnpm instead of npm.",
    "yarn": "Please use pnpm instead of yarn."
  },
  "bugs": {
    "email": "root@AutoQuoteGuide.com",
    "url": "https://github.com/AutoQuoteGuide/lead1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/AutoQuoteGuide/lead1.git"
  },
  "scripts": {
    "dev": "concurrently \"pnpm:vite:dev\" \"pnpm:slicemachine\" --prefix-colors blue,magenta",
    "vite:dev": "vite dev --host --port 3000 --open",
    "build": "vite build --mode ci",
    "preview": "vite preview",
    "lint": "eslint .",
    "slicemachine": "start-slicemachine",
    "lint:fix": "eslint . --fix",
    "db:push": "drizzle-kit push",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "prepare": "node .husky/install.mjs",
    "test:unit": "vitest",
    "test:integration": "playwright test",
    "test": "pnpm run test:integration && pnpm run test:unit",
    "email": "email dev --dir src/components/email",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "icon:generate": "pnpm dlx @vite-pwa/assets-generator --preset minimal-2023 static/favicon.svg"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "@iconify/json": "^2.2.260",
    "@playwright/test": "^1.48.0",
    "@slicemachine/adapter-sveltekit": "^0.3.51",
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/enhanced-img": "^0.3.9",
    "@sveltejs/kit": "^2.7.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/eslint": "^9.6.1",
    "@types/node": "22.7.5",
    "@types/uuid": "^10.0.0",
    "@vite-pwa/sveltekit": "^0.6.5",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.25.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-svelte": "^2.44.1",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "postcss": "8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "slice-machine-ui": "^2.8.1",
    "super-sitemap": "^0.15.1",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animated": "^1.1.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "typescript-svelte-plugin": "^0.3.42",
    "unplugin-icons": "^0.19.3",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-tailwind-purgecss": "0.3.3",
    "vitest": "^2.1.2",
    "workbox-window": "^7.1.0"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.11",
    "@fontsource-variable/archivo": "^5.1.0",
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource-variable/manrope": "^5.1.0",
    "@formkit/auto-animate": "^0.8.2",
    "@internationalized/date": "^3.5.6",
    "@prismicio/client": "^7.11.0",
    "@prismicio/svelte": "^1.3.0",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.34.1",
    "embla-carousel-svelte": "^8.3.0",
    "formsnap": "^1.0.1",
    "highlight.js": "11.10.0",
    "lucide-svelte": "^0.452.0",
    "mode-watcher": "^0.4.1",
    "paneforge": "^0.0.6",
    "postgres": "^3.4.4",
    "svelte-motion": "^0.12.2",
    "svelte-sonner": "^0.3.28",
    "svelte-tel-input": "^3.5.1",
    "sveltekit-superforms": "^2.19.1",
    "tailwind-merge": "^2.5.3",
    "tailwind-variants": "^0.2.1",
    "uuid": "^10.0.0",
    "vaul-svelte": "^0.3.2",
    "zod": "^3.23.8"
  }
}
